{
  "version": 2,
  "builds": [
    {
      "src": "backend/app/main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "backend/app/main.py"
    },
    {
      "src": "/(.*)",
      "dest": "frontend/aura/dist/$1"
    }
  ],
  "functions": {
    "backend/app/main.py": {
      "maxDuration": 30
    }
  }
}
